public class App {
	public static void main(String[] args) {
		Customer nancy = new Customer("Nancy", "Drew", 1234);
		
		Account account = new Account(nancy, 10000);
		account.returnBalance();
		
		account.withDrawl(4000);
		account.returnBalance();
		
		account.addFunds(8000);
		account.returnBalance();
		
		account.withDrawl(4000);
		account.returnBalance();
		
		
		
	}

}
